Config
  { font         = "-*-fixed-medium-*-*-*-*-100-*-*-*-*-*-*"
  , borderColor  = "#dcdccc"
  , border       = TopB
  , bgColor      = "#3f3f3f"
  , fgColor      = "#dcdccc"
  , position     = Bottom R 40
  , lowerOnStart = True
  , commands =
    [ Run Weather "KBWI"
      [ "-t", "<tempF>Â° F"
      , "-L", "40"
      , "-H", "80"
      , "--normal" , "green"
      , "--high"   , "red"
      , "--low"    , "lightblue"
      ] 36000
    , Run Cpu
      [ "-L", "10"
      , "-H", "50"
      , "--normal" , "green"
      , "--high"   , "red"
      ] 10
    , Run Memory ["-t", "Mem: <usedratio>%"] 100
    , Run Swap [] 100
    , Run Date "%a %_d %b %Y %H:%M" "date" 10
    ]
  , sepChar = "%"
  , alignSep = "}{"
  , template = "%cpu% | %memory% * %swap% }{ <fc=#fff796>%date%</fc> | %KBWI%"
  }

